# Copy this file to .env and add your actual API keys

# SkyFi API Key (required for satellite imagery features)
# Get yours at: https://app.skyfi.com/account
SKYFI_API_KEY=your-skyfi-api-key-here

# Weather API Key (optional, for real weather data)
# Get a free key at: https://openweathermap.org/api
WEATHER_API_KEY=your-weather-api-key-here

# Optional: Set spending limits
SKYFI_COST_LIMIT=100
SKYFI_MAX_ORDER_COST=50
